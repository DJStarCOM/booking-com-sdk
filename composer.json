{
    "name": "DJStarCOM/booking-com-sdk",
    "type": "library",
    "description": "Booking.com API SDK",
    "license": "MIT",
    "version": "2.1.4",
    "authors": [
        {
            "name": "Stanislav Tsepeniuk",
            "email": "s.tsepeniuk@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1",
        "php-curl-class/php-curl-class": "^8.5 || ^9.0"
    },
     "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^6.0 || ^7.0",
        "squizlabs/php_codesniffer": "^2.4 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "DJStarCOM\\BookingComSDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "DJStarCOM\\BookingComSDK\\" : "tests"
        }
    }
}
